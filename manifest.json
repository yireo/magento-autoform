{
    "name": "Magento Autoform",
    "manifest_version": 2,
    "version": "2.0",
    "description": "Autocompletes Magento forms",
    "background":{
        "scripts":["background.js"]
    },
    "web_accessible_resources": [
        "fields.js"
    ],
    "browser_action": {
        "default_icon": "icon_19x19.png",
        "default_title": "Autocomplete form"
    },
    "icons": {
        "128": "icon_128x128.png",
        "48": "icon_48x48.png"
    },
    "options_page":"options.html",
    "permissions": [ 
        "tabs", 
        "http://*/*", 
        "https://*/*",
        "storage"
    ]
}
